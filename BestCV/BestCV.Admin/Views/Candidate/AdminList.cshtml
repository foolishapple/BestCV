
@{
    Layout = "_Layout";
    ViewBag.Title = "Quản lý ứng viên";
}
@section Headers{
    <style type="text/css">

        #tableData thead tr th:nth-child(1),
        #tableData tbody tr td:nth-child(1),
        #tableData tfoot tr th:nth-child(1) {
            width: 60px !important;
            max-width: 60px !important;
            min-width: 60px !important;
            text-align: center !important;
        }

        #tableData thead tr th:nth-child(4),
        #tableData tbody tr td:nth-child(4),
        #tableData tfoot tr th:nth-child(4) {
            text-align: center !important;
        }



        .search-control {
            white-space: normal;
            padding-right: 10px;
            padding-left: 10px;
        }

        #btnTableSearch {
            margin-right: 10px;
        }

        .input-group-text {
            cursor: pointer;
        }

        #tableData td {
            vertical-align: middle;
        }

        .column-type, .column-status {
            width: 140px !important;
            max-width: 140px !important;
            min-width: 140px !important;
        }

        #candidatePhoto {
            width: 140px;
            height: 140px;
            border-radius: 5px;
            object-position: center;
            object-fit: cover;
        }

        #candidateModal .select2-selection__choice__remove {
            display: none;
        }

        #candidateModal .select2-selection__choice__display {
            margin-left: unset !important;
        }

        .advanced-search{
            padding: 2rem 2.25rem 0 !important;
        }
    </style>
}
